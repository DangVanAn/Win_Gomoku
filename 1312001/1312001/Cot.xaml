                <RowDefinition Name="C0_R9" Height="*" />
                <RowDefinition Name="C0_R10" Height="*" />
                <RowDefinition Name="C0_R11" Height="*" />
            </Grid.RowDefinitions>
            <Button Width="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" 
                    Height="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" Name="x0_y0" Grid.Column="0" Grid.Row="0" Click="x0_y0_Click" />               
            <Button Width="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" 
                    Height="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" Name="x1_y0" Grid.Column="0" Grid.Row="1" Click="x1_y0_Click"/>
