            <Button Width="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" 
                    Height="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" Name="x4_y0" Grid.Column="0" Grid.Row="4" Click="x4_y0_Click"/>
            <Button Width="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" 
                    Height="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" Name="x5_y0" Grid.Column="0" Grid.Row="5" Click="x5_y0_Click"/>
