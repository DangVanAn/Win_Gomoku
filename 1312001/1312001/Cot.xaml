                    Height="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" Name="x8_y0" Grid.Column="0" Grid.Row="8" Click="x8_y0_Click"/>
            <Button Width="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" 
                    Height="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" Name="x9_y0" Grid.Column="0" Grid.Row="9" Click="x9_y0_Click"/>
            <Button Width="{Binding ElementName=C0_C0, Path=Width, Mode=OneWay}" 
